echo " ${a}╔═══════════════╗"
echo "\033[1;90m |\033[1;97mWITH LOGIN   \033[1;90m  |"
echo " ${a}╚═══════════════╝"
echo "${p}[${h}05${p}] ${c} LOGIN WITH FACEBOOK ${p}「NEW」"
echo "${p}[${h}06${p}] ${h} TARGET ACCOUNT HACK${p}「NEW」"
echo
echo " ${a}╔═══════════════╗"
echo "\033[1;90m |\033[1;97mOTHERS            \033[1;90m  |"
echo " ${a}╚═══════════════╝"
echo "${p}[${h}07${p}] ${c} WHATSAPP VIRUS${p}「NEW」"
echo "${p}[${h}08${p}] ${K} HACK FB GROUP HACK${p}「NEW」"
echo "${p}[${h}00${p}] ${p2}CLOSE YOUR PROGRAM"
echo "${k}<═════════════════════════════════════════════>"
echo
echo "${p}"
echo " ${a}╔══════╗"
read -p " |SELECT|" r
echo " ╚══════╝"
if [ $r = 01 ] || [ $r = 1 ];then
sleep 1
python2 plo
sleep 2
cd Dashing-boy
python2 Dashing-boy.py
sleep 1
clear

fi

if [ $r = 02 ] || [ $r = 2 ];then
sleep 2
python2 plo
sleep 2
cd  digit
sh digit.sh
