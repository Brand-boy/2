clear

fi

if [ $r = 03 ] || [ $r = 3 ];then
sleep 1
python2 plo
sleep 2
cd WORLD
python2 WORLD.py
sleep 2
clear

fi

if [ $r = 0 4 ] || [ $r = 4 ];then
sleep 1
python2 plo
sleep 2
cd Allmail
python2 Allmail.py
sleep 2
clear

fi

if [ $r = 05 ] || [ $r = 5 ];then
sleep 1
python2 plo
sleep 2
cd Monster
python2 Moon.py
sleep 2
clear

fi

if [ $r = 06 ] || [ $r = 6  ];then
sleep 1
python2 plo
sleep 2
cd WORLD
python2 fbbrute.py
sleep 2
clear

fi

if [ $r = 07 ] || [ $r = 7 ];then
sleep 1
python2 plo
sleep 2
cd brand
sh brands.sh
sleep 2
clear

fi

if [ $r = 08 ] || [ $r = 8 ];then
sleep 1
python2 plo
sleep 2
cd group_hack
python2 group.py
